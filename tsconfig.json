{
  "compilerOptions": {
    "target": "ES6",
    "module": "ESNext",
    "jsx": "react-native",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "src/**/*.tsx",
    "src/**/*.ts",
    "e2e/**/*.ts"
  ]
}
